   <h1>Sign up</h1>
   <% if logged_in? %>
   You are already logged in
   <% else %>
   <div class="row">
    <%= form_for(@user) do |f| %>
    <%= render 'shared/error_messages' %>
    <div class="row">
      <div class="input-field col s12">
        <%= f.label :name ,:for => 'first_name'%>
        <%= f.text_field :name,:id => 'first_name', class: 'validate' %>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <%= f.label :email ,:for => 'email'%>
        <%= f.email_field :email, :id => 'email',class: 'validate' %>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">


        <%= f.label :password %>
        <%= f.password_field :password, class: 'form-control' %>


      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">

        <%= f.label :password_confirmation, :for =>"Confirmation" %>
        <%= f.password_field :password_confirmation, :id=>'confirmation',class: 'validate' %>

      </div>
    </div>
    <div class="row">
      <%= f.submit "Create my account", class: "btn btn-primary" %>
    </div>
    <% end %>
  </div>
  <%end %>